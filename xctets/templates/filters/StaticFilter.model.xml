<?xml version="1.0" encoding="utf-8"?>
<model name="static filter" >

	<description>
	Filter that is applied on every request
	</description>
				
	<gen_class type="standard" ns="filters" path="filters">
	</gen_class>

	<var_group genGet="true" genSet="true" construct="true">
		<var name="column"  type="string" />
		<var name="value"	type="string" />
	</var_group>		
	
</model>
