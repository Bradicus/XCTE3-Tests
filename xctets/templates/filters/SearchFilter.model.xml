<?xml version="1.0" encoding="utf-8"?>
<model name="search filter" >

	<description>
	Filters data set based on search
	</description>
				
	<gen_class type="standard" ns="filters" path="filters">
	</gen_class>

	<var_group genGet="true" genSet="true" construct="true">
		<var name="name"	type="string" default="search" />
		<var name="type"	type="string" default="shared" />
		<var name="columns"	type="Int32" set="List" />
	</var_group>		
	
</model>
