<?xml version="1.0" encoding="utf-8"?>
<model name="data filter" >

	<description>
	Filters request for sets of data
	</description>
				
	<gen_class type="standard" ns="filters" path="filters">
	</gen_class>

	<var_group genGet="true" genSet="true" construct="true">
		<var name="paging"	utype="paging filter" ns="filters" />
		<var name="searches" utype="search filter" ns="filters" set="List" />
		<var name="sort"	utype="sort filter" ns="filters" />
		<var name="static filter"	utype="static filter" ns="filters" />
	</var_group>		
	
</model>
