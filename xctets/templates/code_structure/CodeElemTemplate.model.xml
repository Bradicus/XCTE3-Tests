<?xml version="1.0" encoding="utf-8"?>
<model name="code elem template" >

	<description>
	Code Element for Templates or in some languages arrays
	</description>

	<gen_class type="standard" ns="code structure" path="code structure" >
		<base_class cname="code elem" plugin="standard" />

		<function name="method_constructor" >
			<param name="tpl string" type="String" />
		</function>
	</gen_class>

	<var_group genGet="true" genSet="true" construct="true">		
		<var name="name"			type="string" />        
        <var name="is pointer tpl"	type="boolean" default="false" />        
        <var name="is collection"	type="boolean" default="false" />
	</var_group>

</model>
